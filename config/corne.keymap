#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
        keymap {
                compatible = "zmk,keymap";

                default_layer {
                        display-name = "Default";
                        bindings = <
// ╭──────────┬──────────┬──────────┬──────────┬──────────┬──────────╮ ╭──────────┬──────────┬──────────┬──────────┬──────────┬──────────╮
// | TAB      | Q        | W        | E        | R        | T        | | Y        | U        | I        | O        | P        | ESC      |
    &kp TAB    &kp Q      &kp W      &kp E      &kp R      &kp T        &kp Y      &kp U      &kp I      &kp O      &kp P      &kp ESC
// ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤ ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤
// | CTRL     | A        | S        | D        | F        | G        | | H        | J        | K        | L        | ;        | ENT      |
    &kp LCTRL  &kp A      &kp S      &kp D      &kp F      &kp G        &kp H      &kp J      &kp K      &kp L      &kp SEMI   &kp RET
// ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤ ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤
// | SHFT     | Z        | X        | C        | V        | B        | | N        | M        | ,        | .        | /        | ALT      |
    &kp LSHFT  &kp Z      &kp X      &kp C      &kp V      &kp B        &kp N      &kp M      &kp COMMA  &kp DOT    &kp FSLH   &kp LALT
// ╰──────────┴──────────┴──────────┼──────────┼──────────┼──────────┤ ├──────────┼──────────┼──────────┼──────────┴──────────┴──────────╯
//                                  | GUI      | LWR      | BSPC     | | SPC      | RSE      | HYPER    |
                                     &kp LGUI   &mo 1      &kp BSPC     &kp SPACE  &mo 2      &kp LC(LA(LS(LGUI)))
//                                  ╰──────────┴──────────┴──────────╯ ╰──────────┴──────────┴──────────╯
                        >;
                };
                lower_layer {
                        display-name = "Lower L";
                        bindings = <
// ╭──────────┬──────────┬──────────┬──────────┬──────────┬──────────╮ ╭──────────┬──────────┬──────────┬──────────┬──────────┬──────────╮
// | TAB      | 1        | 2        | 3        | 4        | 5        | | 6        | 7        | 8        | 9        | 0        | ESC      |
    &kp TAB    &kp N1     &kp N2     &kp N3     &kp N4     &kp N5       &kp N6     &kp N7     &kp N8     &kp N9     &kp N0     &kp ESC
// ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤ ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤
// | CTRL     | CAPS     | PSCRN    |          |          |          | |          | LFT      | DWN      | UP       | RGT      | ENT      |
    &kp LCTRL  &kp CAPS   &kp PSCRN  &trans     &trans     &trans       &trans     &kp LEFT   &kp DOWN   &kp UP     &kp RIGHT  &kp RET
// ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤ ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤
// | SHFT     |          |          |          |          |          | |          |          |          |          |          | ALT      |
    &kp LSHFT  &trans     &trans     &trans     &trans     &trans       &trans     &trans     &trans     &trans     &trans     &kp LALT
// ╰──────────┴──────────┴──────────┼──────────┼──────────┼──────────┤ ├──────────┼──────────┼──────────┼──────────┴──────────┴──────────╯
//                                  | GUI      |          | BSPC     | | SPC      | TRI      | HYPER    |
                                     &kp LGUI   &trans     &kp BSPC     &kp SPACE  &mo 3      &kp LC(LA(LS(LGUI)))
//                                  ╰──────────┴──────────┴──────────╯ ╰──────────┴──────────┴──────────╯
                        >;
                };

                raise_layer {
                        display-name = "Raise L";
                        bindings = <
// ╭──────────┬──────────┬──────────┬──────────┬──────────┬──────────╮ ╭──────────┬──────────┬──────────┬──────────┬──────────┬──────────╮
// | TAB      | !        | @        | #        | $        | %        | | ^        | &        | *        | ~        | `        | ESC      |
    &kp  TAB   &kp EXCL   &kp AT     &kp HASH   &kp DLLR   &kp PRCNT    &kp CARET  &kp AMPS   &kp ASTRK  &kp TILDE  &kp GRAVE  &kp ESC
// ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤ ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤
// | CTRL     |          | \        | {        | (        | +        | | -        | )        | }        | '        |          | ENT      |
    &kp LCTRL  &trans     &kp BSLH   &kp LBRC   &kp LPAR   &kp PLUS     &kp MINUS  &kp RPAR   &kp RBRC   &kp SQT    &trans     &kp RET
// ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤ ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤
// | SHFT     |          |          | |        | [        | =        | | _        | ]        | "        |          |          | ALT      |
    &kp LSHFT  &trans     &trans     &kp PIPE   &kp LBKT   &kp EQUAL    &kp UNDER  &kp RBKT   &kp DQT    &trans     &trans     &kp LALT
// ╰──────────┴──────────┴──────────┼──────────┼──────────┼──────────┤ ├──────────┼──────────┼──────────┼──────────┴──────────┴──────────╯
//                                  | GUI      | TRI      | BSPC     | | SPC      |          | HYPER    |
                                     &kp LGUI   &mo 3      &kp BSPC     &kp SPACE  &trans     &kp LC(LA(LS(LGUI)))
//                                  ╰──────────┴──────────┴──────────╯ ╰──────────┴──────────┴──────────╯
                        >;
                };

                tri_layer {
                        display-name = "Tri L";
                        bindings = <
// ╭──────────┬──────────┬──────────┬──────────┬──────────┬──────────╮ ╭──────────┬──────────┬──────────┬──────────┬──────────┬──────────╮
// | F1       | F2       | F3       | F4       | F5       | F6       | | F7       | F8       | F9       | F10      | F11      | F12      |
    &kp  F1    &kp F2     &kp F3     &kp F4     &kp F5     &kp F6       &kp F7     &kp F8     &kp F9     &kp F10    &kp F11    &kp F12
// ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤ ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤
// | CTRL     | BT1      | BT2      | BT3      | BT4      | BT5      | | BRI_UP   | VOL_UP   | PLAY     | PREV     |          | ENT      |
    &kp LCTRL &bt BT_SEL 0 &bt BT_SEL 1 &bt BT_SEL 2 &bt BT_SEL 3 &bt BT_SEL 4 &kp C_BRI_UP &kp C_VOL_UP &kp C_PP &kp C_PREV &trans &kp RET
// ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤ ├──────────┼──────────┼──────────┼──────────┼──────────┼──────────┤
// | SHFT     | CLEAR BT |          |          |          |          | | BRI_DOWN | VOL_DOWN | VOL_MUTE | NEXT     |          | ALT      |
    &kp LSHFT  &bt BT_CLR &trans     &trans     &trans     &trans       &kp C_BRI_DN &kp C_VOL_DN &kp C_MUTE &kp C_NEXT &trans &kp LALT
// ╰──────────┴──────────┴──────────┼──────────┼──────────┼──────────┤ ├──────────┼──────────┼──────────┼──────────┴──────────┴──────────╯
//                                  | GUI      |          | BSPC     | | SPC      |          | HYPER    |
                                     &kp LGUI   &trans     &kp BSPC     &kp SPACE  &trans     &kp LC(LA(LS(LGUI)))
//                                  ╰──────────┴──────────┴──────────╯ ╰──────────┴──────────┴──────────╯
                        >;
                };
        };
};
